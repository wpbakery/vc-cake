    "modules"
  ],
  "author": "Slava Vasenin <slava@visualcomposer.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wpbakery/vc-cake/issues"
  },
  "homepage": "https://github.com/wpbakery/vc-cake#readme",
  "dependencies": {
    "immutable": "4.0.0",
    "lodash": "4.17.21",
    "murmurhash": "2.0.0"
  },
  "devDependencies": {
    "husky": "7.0.4",
    "jest-cli": "27.4.7",
    "jscs": "3.0.7",
    "standard": "16.0.4"
  },
  "jest": {
